msgid "Extruder"
msgstr "Extrusora"

#: src/libslic3r/PrintConfig.cpp:936 src/libslic3r/PrintConfig.cpp:1617
#: src/libslic3r/PrintConfig.cpp:2133 src/libslic3r/PrintConfig.cpp:2546
