msgstr "Monotonisch"

#: src/libslic3r/PrintConfig.cpp:871
msgid "Monotonic Lines"
msgstr "Monotone Linien"

#: src/libslic3r/PrintConfig.cpp:872 src/libslic3r/PrintConfig.cpp:901
#: src/libslic3r/PrintConfig.cpp:1372
msgid "Aligned Rectilinear"
msgstr "Ausgerichtet Geradlinig"

#: src/libslic3r/PrintConfig.cpp:873 src/libslic3r/PrintConfig.cpp:899
#: src/libslic3r/PrintConfig.cpp:1378 src/libslic3r/PrintConfig.cpp:3040
