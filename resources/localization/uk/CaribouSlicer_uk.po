#: src/libslic3r/PrintConfig.cpp:873 src/libslic3r/PrintConfig.cpp:1343
msgid "Hilbert Curve"
msgstr "Крива Гільберта"

#: src/libslic3r/PrintConfig.cpp:874 src/libslic3r/PrintConfig.cpp:1344
msgid "Archimedean Chords"
msgstr "Архімедові акорди"

#: src/libslic3r/PrintConfig.cpp:875 src/libslic3r/PrintConfig.cpp:1345
